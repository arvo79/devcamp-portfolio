<p>
	<%= link_to image_tag(portfolio_item.thumb_image), portfolio_show_path(portfolio_item) unless portfolio_item.thumb_image.nil? %>
	<%= portfolio_item.title %>
	<%= portfolio_item.subtitle %>
	<%= portfolio_item.body %>
	<%= link_to 'Show', portfolio_show_path(portfolio_item) %>
	<%= link_to 'Edit', edit_portfolio_path(portfolio_item) %>
	<%= link_to 'Delete', portfolio_item, method: :delete, data: { confirm: 'Delete Portfolio Item?' } %>
</p>